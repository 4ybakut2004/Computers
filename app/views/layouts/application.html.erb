<!DOCTYPE html>
<html>
<head>
  <%= render 'layouts/meta' %>
  <title>Ремонт компьютеров</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <link rel="apple-touch-icon" sizes="180x180" href="<%= asset_path('fav/apple-touch-icon.png') %>">
  <link rel="icon" type="image/png" sizes="32x32" href="<%= asset_path('fav/favicon-32x32.png') %>">
  <link rel="icon" type="image/png" sizes="16x16" href="<%= asset_path('fav/favicon-16x16.png') %>">
  <link rel="manifest" href="<%= asset_path('fav/manifest.json') %>">
  <link rel="mask-icon" href="asset_path('fav/safari-pinned-tab.svg') %>" color="#5bbad5">
  <meta name="theme-color" content="#ffffff">
  <%= render 'layouts/ie' %>
</head>
<body
  ng-controller="HomeController"
  ng-strict-di
  id="App"
  class="homepage">
  <div>
    <%= yield %>
  </div>
</body>
</html>